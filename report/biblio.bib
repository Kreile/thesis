	Journal = {Controlled Clinical Trials},
	Keywords = {random effects model, heterogeneity of treatment effects, distribution of treatment effects, covariate information},
	Number = {3},
	Pages = {177 - 188},
	Title = {Meta-analysis in clinical trials},
