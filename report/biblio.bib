@article{Copas2,
	Abstract = {{Publication bias is a major problem, perhaps the major problem, in meta-analysis (or systematic reviews). Small
 studies are more likely to be published if their results are
 `significant' than if their results are negative or
 inconclusive, and so the studies available for review are biased in
 favour of those with positive outcomes. Correcting for this bias is
 not possible without making untestable assumptions. In this paper, a
 sensitivity analysis is suggested which is based on fitting a model
 to the funnel plot. Some examples are discussed.*To whom correspondence should be addressed. jbc@stats.warwick.ac.uk}},
	Author = {Copas, John and Shi, Jian Qing},
	Date-Added = {2019-04-09 14:22:23 +0200},
	Date-Modified = {2019-04-09 14:22:31 +0200},
	Doi = {10.1093/biostatistics/1.3.247},
	Eprint = {http://oup.prod.sis.lan/biostatistics/article-pdf/1/3/247/725192/010247.pdf},
	Issn = {1465-4644},
	Journal = {Biostatistics},
	Month = {09},
	Number = {3},
	Pages = {247-262},
	Title = {{Meta-analysis, funnel plots and sensitivity analysis }},
	Url = {https://doi.org/10.1093/biostatistics/1.3.247},
	Volume = {1},
	Year = {2000},
	Bdsk-Url-1 = {https://doi.org/10.1093/biostatistics/1.3.247}}

@article{Copas1,
	Abstract = { There is no simple method of correcting for publication bias in systematic reviews. We suggest a sensitivity analysis in which different patterns of selection bias can be tested against the fit to the funnel plot. Publication bias leads to lower values, and greater uncertainty, in treatment effect estimates. Two examples are discussed. An appendix lists the S-plus code needed for carrying out the analysis. },
	Author = {J B Copas and J Q Shi},
	Date-Added = {2019-04-09 14:21:11 +0200},
