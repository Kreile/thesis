	Journal = {Journal of Clinical Epidemiology},
	Keywords = {Publication bias, Full publication, Dissemination bias, Underreporting, Statistical methods, Small-study effect, The OPEN project},
	Pages = {25 - 33},
	Title = {Methods for detecting, quantifying, and adjusting for dissemination bias in meta-analysis are described},
