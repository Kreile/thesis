	Journal = {PLOS ONE},
	Month = {07},
	Number = {7},
	Pages = {1-37},
	Publisher = {Public Library of Science},
	Title = {Systematic Review of the Empirical Evidence of Study Publication Bias and Outcome Reporting Bias --- An Updated Review},
