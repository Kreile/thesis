	Issn = {0959-8138},
	Journal = {BMJ},
	Number = {7109},
	Pages = {629--634},
	Publisher = {BMJ Publishing Group Ltd},
	Title = {Bias in meta-analysis detected by a simple, graphical test},
	Url = {https://www.bmj.com/content/315/7109/629},
	Volume = {315},
	Year = {1997},
	Bdsk-Url-1 = {https://www.bmj.com/content/315/7109/629},
